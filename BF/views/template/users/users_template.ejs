<script type="text/template" id="users_template">
    {{each list}}
      <tr>
        <td class="text-center"><input type="checkbox" class="item-checked" data-id='{{$value.id}}'></td>
        <td>{{$value.email}}</td>
        <td>{{$value.firstName}}</td>
        <td>{{$value.lastName}}</td>
        <td>{{$value.title}}</td>
        <td class="text-center">
          <a href="javascript:;" data-id='{{$value.id}}' class="btn btn-default btn-xs">编辑</a>
          <a href="javascript:;" data-id='{{$value.id}}' class="btn btn-danger btn-xs">删除</a>
        </td>
      </tr>
    {{/each}}
</script>