<script>
    var aSpan = document.querySelectorAll('.Lips span');
    var aSpan2 = document.querySelectorAll('.hong span')
    for (i = 0; i < aSpan.length; i++) {
        aSpan[i].setAttribute('index', i)
        aSpan[i].onmouseenter = function () {
            for (j = 0; j < aSpan.length; j++) {
                aSpan2[j].style.zIndex = 0;
            }
            aSpan2[this.getAttribute('index')].style.zIndex = 10;
        }
    }
</script>

