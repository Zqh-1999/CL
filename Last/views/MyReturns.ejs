<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>我的回报</title>
    <link rel="stylesheet" href="/css/MyReturns.css">
    <!-- 链接 -->
    <%- include('./layouts/hearderLink.ejs')%>
</head>

<body>
    <!-- 顶部导航 -->
    <%- include('./layouts/header.ejs')%>

    <!-- 中间部分 top -->
    <div class="content clearfix">
        <!-- 标题 -->
        <div class="page-header">
            <h1><%- include('./template/userName.ejs')%></h1>
            <div class="hr"></div>
        </div>

        <!-- 左边 导航栏 -->
        <%- include('./layouts/navbar.ejs')%>


        <!--右边  -->
        <div class="wrap">
            <div class="wrapper-grid1">
                <h2>我的回报</h2>
                <div class="hr hrtwo"></div>
            </div>
            <div class="order-center">
                <P>你没有退货</P>
            </div>
        </div>
        <!-- 遮罩层 start -->
        <%- include('./layouts/masklayer.ejs')%>
    </div>

    <!-- 底部 -->
    <%- include('./layouts/footer.ejs')%>

    <!-- 底部链接 -->
    <%- include('./layouts/footerLink.ejs')%>

    <script>
        function show() {
            $.ajax({
                type: 'get',
                url: '/admin/showNameingse',
                data: '',
                success: function (data) {
                    let nameDate = template('userName_template', data[0])
                    $('.page-header h1').text(nameDate)
                    console.log(nameDate)
                }
            })
        }
        show()
    </script>

</body>

</html>